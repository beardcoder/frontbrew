{
    "name": "frontbrew",
    "version": "1.0.0",
    "description": "Build pipeline for neos projects",
    "private": true,
    "scripts": {
        "dep-install": "yarn --cwd $BASE_PATH/$PROJECT_PRIVATE",
        "start": "yarn run dep-install && yarn run start-dev-server",
        "start-dev-server": "webpack-dev-server --color --hot",
        "start-watch": "webpack --watch",
        "build": "yarn run dep-install && webpack --hide-modules --process",
        "stylelint": "stylelint \"$BASE_PATH/$PROJECT_PRIVATE/**/*.scss\"",
        "eslint": "eslint --ignore-path=$BASE_PATH/$PROJECT_PRIVATE/.eslintignore \"$BASE_PATH/$PROJECT_PRIVATE/**/*.js\""
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@babel/core": "~7.4.5",
        "@babel/plugin-proposal-class-properties": "~7.4.4",
        "@babel/plugin-proposal-object-rest-spread": "~7.4.4",
        "@babel/plugin-syntax-dynamic-import": "~7.2.0",
        "@babel/plugin-transform-runtime": "~7.4.4",
        "@babel/preset-env": "~7.4.5",
        "@babel/preset-typescript": "^7.3.3",
        "@types/autoprefixer": "^9.5.0",
        "@types/cssnano": "^4.0.0",
        "@types/node": "^12.0.2",
        "@types/webpack": "^4.4.32",
        "@types/webpack-dev-server": "^3.1.5",
        "autoprefixer": "^9.5.1",
        "babel-loader": "~8.0.6",
        "babel-minify-webpack-plugin": "~0.3.1",
        "cache-loader": "^3.0.1",
        "clean-webpack-plugin": "^2.0.2",
        "css-loader": "~2.1.1",
        "cssnano": "~4.1.10",
        "eslint": "5.16.0",
        "eslint-config-airbnb": "17.1.0",
        "eslint-friendly-formatter": "~4.0.1",
        "eslint-loader": "~2.1.2",
        "eslint-plugin-import": "^2.17.2",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.13.0",
        "install": "~0.12.2",
        "mini-css-extract-plugin": "^0.6.0",
        "node-sass": "~4.12.0",
        "node-sass-glob-importer": "~5.3.1",
        "optimize-css-assets-webpack-plugin": "~5.0.1",
        "postcss-cli": "~6.1.2",
        "postcss-color-function": "~4.1.0",
        "postcss-import": "~12.0.1",
        "postcss-loader": "~3.0.0",
        "postcss-mixins": "~6.2.1",
        "postcss-preset-env": "~6.6.0",
        "postcss-reporter": "^6.0.1",
        "precss": "^4.0.0",
        "raw-loader": "^2.0.0",
        "sass-loader": "~7.1.0",
        "source-map-loader": "^0.2.4",
        "style-loader": "~0.23.1",
        "stylelint": "^10.0.1",
        "stylelint-declaration-use-variable": "~1.7.0",
        "stylelint-scss": "~3.6.1",
        "stylelint-webpack-plugin": "~0.10.5",
        "ts-node": "^8.1.0",
        "typescript": "^3.4.5",
        "webpack": "~4.32.2",
        "webpack-cli": "~3.3.2",
        "webpack-dev-server": "~3.4.1",
        "webpack-merge": "~4.2.1",
        "webpack-stylish": "~0.1.8"
    }
}
