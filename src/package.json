{
    "name": "frontbrew",
    "version": "1.0.0",
    "description": "Build pipeline for neos projects",
    "private": true,
    "scripts": {
        "dep-install": "yarn --cwd $BASE_PATH/$PROJECT_PRIVATE",
        "start": "yarn run dep-install && yarn run start-dev-server",
        "start-dev-server": "webpack-dev-server --color --hot",
        "start-watch": "webpack --watch",
        "build": "yarn run dep-install && webpack --hide-modules --process",
        "stylelint": "stylelint \"$BASE_PATH/$PROJECT_PRIVATE/**/*.scss\"",
        "eslint": "eslint --ignore-path=$BASE_PATH/$PROJECT_PRIVATE/.eslintignore \"$BASE_PATH/$PROJECT_PRIVATE/**/*.js\""
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@babel/core": "7.10.5",
        "@babel/plugin-proposal-class-properties": "7.10.4",
        "@babel/plugin-proposal-object-rest-spread": "7.10.4",
        "@babel/plugin-syntax-dynamic-import": "7.8.3",
        "@babel/plugin-transform-runtime": "7.10.5",
        "@babel/preset-env": "7.10.4",
        "@babel/preset-typescript": "7.10.4",
        "@types/autoprefixer": "9.7.2",
        "@types/cssnano": "4.0.0",
        "@types/node": "13.13.14",
        "@types/webpack": "4.41.21",
        "@types/webpack-dev-server": "3.11.0",
        "@typescript-eslint/eslint-plugin": "3.7.0",
        "autoprefixer": "9.8.5",
        "babel-loader": "8.1.0",
        "babel-minify-webpack-plugin": "0.3.1",
        "cache-loader": "4.1.0",
        "css-loader": "3.6.0",
        "cssnano": "4.1.10",
        "eslint": "7.5.0",
        "eslint-config-airbnb": "18.2.0",
        "eslint-friendly-formatter": "4.0.1",
        "eslint-loader": "4.0.2",
        "eslint-plugin-import": "2.22.0",
        "eslint-plugin-jsx-a11y": "6.3.1",
        "eslint-plugin-prettier": "3.1.4",
        "eslint-plugin-react": "7.20.3",
        "install": "0.13.0",
        "mini-css-extract-plugin": "0.9.0",
        "node-sass": "7.0.0",
        "node-sass-glob-importer": "5.3.2",
        "optimize-css-assets-webpack-plugin": "5.0.3",
        "postcss-cli": "7.1.1",
        "postcss-color-function": "4.1.0",
        "postcss-import": "12.0.1",
        "postcss-loader": "3.0.0",
        "postcss-mixins": "6.2.3",
        "postcss-preset-env": "6.7.0",
        "postcss-reporter": "6.0.1",
        "precss": "4.0.0",
        "raw-loader": "4.0.1",
        "sass-loader": "9.0.2",
        "source-map-loader": "1.0.1",
        "style-loader": "1.2.1",
        "stylelint": "13.6.1",
        "stylelint-declaration-use-variable": "1.7.2",
        "stylelint-scss": "3.18.0",
        "stylelint-webpack-plugin": "2.1.0",
        "ts-node": "8.10.2",
        "typescript": "3.9.7",
        "webpack": "4.43.0",
        "webpack-cli": "3.3.12",
        "webpack-dev-server": "3.11.0",
        "webpack-merge": "5.0.9"
    },
