{
    "name": "frontbrew",
    "version": "1.0.0",
    "description": "Build pipeline for neos projects",
    "private": true,
    "scripts": {
        "dep-install": "yarn --cwd $BASE_PATH/$PROJECT_PRIVATE",
        "start": "yarn run dep-install && yarn run start-dev-server",
        "start-dev-server": "webpack-dev-server --color --hot",
        "start-watch": "webpack --watch",
        "build": "yarn run dep-install && webpack --hide-modules --process",
        "stylelint": "stylelint \"$BASE_PATH/$PROJECT_PRIVATE/**/*.scss\"",
        "eslint": "eslint --ignore-path=$BASE_PATH/$PROJECT_PRIVATE/.eslintignore \"$BASE_PATH/$PROJECT_PRIVATE/**/*.js\""
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@babel/core": "7.8.0",
        "@babel/plugin-proposal-class-properties": "7.7.4",
        "@babel/plugin-proposal-object-rest-spread": "7.7.7",
        "@babel/plugin-syntax-dynamic-import": "7.7.4",
        "@babel/plugin-transform-runtime": "7.8.0",
        "@babel/preset-env": "7.7.7",
        "@babel/preset-typescript": "7.7.7",
        "@types/autoprefixer": "9.6.1",
        "@types/cssnano": "4.0.0",
        "@types/node": "13.1.6",
        "@types/webpack": "4.41.2",
        "@types/webpack-dev-server": "3.9.0",
        "@typescript-eslint/eslint-plugin": "2.16.0",
        "autoprefixer": "9.7.3",
        "babel-loader": "8.0.6",
        "babel-minify-webpack-plugin": "0.3.1",
        "cache-loader": "4.1.0",
        "css-loader": "3.4.2",
        "cssnano": "4.1.10",
        "eslint": "6.8.0",
        "eslint-config-airbnb": "18.0.1",
        "eslint-friendly-formatter": "4.0.1",
        "eslint-loader": "3.0.3",
        "eslint-plugin-import": "2.20.0",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "eslint-plugin-react": "7.18.0",
        "install": "0.13.0",
        "mini-css-extract-plugin": "0.9.0",
        "node-sass": "4.13.1",
        "node-sass-glob-importer": "5.3.2",
        "optimize-css-assets-webpack-plugin": "5.0.3",
        "postcss-cli": "7.1.0",
        "postcss-color-function": "4.1.0",
        "postcss-import": "12.0.1",
        "postcss-loader": "3.0.0",
        "postcss-mixins": "6.2.3",
        "postcss-preset-env": "6.7.0",
        "postcss-reporter": "6.0.1",
        "precss": "4.0.0",
        "raw-loader": "4.0.0",
        "sass-loader": "8.0.2",
        "source-map-loader": "0.2.4",
        "style-loader": "1.1.1",
        "stylelint": "13.0.0",
        "stylelint-declaration-use-variable": "1.7.2",
        "stylelint-scss": "3.13.0",
        "stylelint-webpack-plugin": "1.2.1",
        "ts-node": "8.6.2",
        "typescript": "3.7.5",
        "webpack": "4.41.5",
        "webpack-cli": "3.3.10",
        "webpack-dev-server": "3.10.1",
        "webpack-merge": "4.2.2"
    }
}
