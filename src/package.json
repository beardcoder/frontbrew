{
    "name": "frontbrew",
    "version": "1.0.0",
    "description": "Build pipeline for neos projects",
    "private": true,
    "scripts": {
        "dep-install": "npm --prefix $BASE_PATH/$PROJECT_PRIVATE install",
        "start": "npm run dep-install && npm run start-dev-server",
        "start-dev-server": "webpack-dev-server --color --hot",
        "start-watch": "webpack --watch",
        "build": "npm run dep-install && webpack --hide-modules --process",
        "stylelint": "stylelint \"$BASE_PATH/$PROJECT_PRIVATE/**/*.scss\"",
        "eslint": "eslint --ignore-path=$BASE_PATH/$PROJECT_PRIVATE/.eslintignore \"$BASE_PATH/$PROJECT_PRIVATE/**/*.js\""
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@babel/core": "~7.3.4",
        "@babel/plugin-proposal-class-properties": "~7.3.4",
        "@babel/plugin-proposal-object-rest-spread": "~7.3.4",
        "@babel/plugin-syntax-dynamic-import": "~7.2.0",
        "@babel/plugin-transform-runtime": "~7.3.4",
        "@babel/preset-env": "~7.3.4",
        "@babel/preset-typescript": "^7.3.3",
        "autoprefixer": "~9.4.10",
        "babel-loader": "~8.0.5",
        "babel-minify-webpack-plugin": "~0.3.1",
        "cache-loader": "^2.0.1",
        "css-loader": "~2.1.1",
        "cssnano": "~4.1.10",
        "eslint": "^5.15.1",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-friendly-formatter": "~4.0.1",
        "eslint-loader": "~2.1.2",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.12.4",
        "extract-css-chunks-webpack-plugin": "~4.0.1",
        "install": "~0.12.2",
        "node-sass": "~4.11.0",
        "node-sass-glob-importer": "~5.3.0",
        "optimize-css-assets-webpack-plugin": "~5.0.1",
        "postcss-cli": "~6.1.2",
        "postcss-color-function": "~4.0.1",
        "postcss-import": "~12.0.1",
        "postcss-loader": "~3.0.0",
        "postcss-mixins": "~6.2.1",
        "postcss-preset-env": "~6.6.0",
        "precss": "^4.0.0",
        "raw-loader": "^1.0.0",
        "sass-loader": "~7.1.0",
        "source-map-loader": "^0.2.4",
        "style-loader": "~0.23.1",
        "stylelint": "^9.10.1",
        "stylelint-declaration-use-variable": "~1.7.0",
        "stylelint-scss": "~3.5.4",
        "stylelint-webpack-plugin": "~0.10.5",
        "typescript": "^3.4.0-dev.20190311",
        "webpack": "~4.29.6",
        "webpack-cli": "~3.2.3",
        "webpack-dev-server": "~3.2.1",
        "webpack-merge": "~4.2.1",
        "webpack-stylish": "~0.1.8"
    }
}
